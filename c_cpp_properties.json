{
    "configurations": [
        {
            "name": "win64_d",
            "includePath": [
                ".",
                "${env:INCLUDE}",
                "${env:PYTHONPATH}/libs",
                "${workspaceFolder}/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "compilerPath": "${env:USRLOCAL}/bin/clang++.exe",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "windows-clang-x64",
            "configurationProvider": "ms-vscode.cmake-tools"
        },
        {
            "name": "linux64_d",
            "includePath": [
                ".",
                "/usr/include/python3.9m",
                "/usr/include/python3.8m",
                "/usr/include/python3.7m",
                "/usr/include/python3.6m",
                "/usr/include/python2.7m",
                "/usr/include/boost",
                "/usr/local/include",
                "/usr/include",
                "${workspaceFolder}/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "compilerPath": "/usr/bin/clang++",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-clang-x64"
        }
    ],
    "version": 4
}
